{
  "emulators": {
    "singleProjectMode": false,
    "hosting": {
      "host": "0.0.0.0",
      "port": 5001
    },
    "firestore": {
      "port": 8080,
      "host": "0.0.0.0"
    },
    "ui": {
      "enabled": true
    },
    "auth": {
      "port": 9099,
      "host": "0.0.0.0"
    },
    "storage": {
      "port": 9199,
      "host": "0.0.0.0"
    }
  },
  "remoteconfig": {
    "template": "remote_config_defaults.json"
  },
  "storage": {
    "rules": "storage.rules"
  },
  "hosting": {
    "public": "public",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
        "source": "**",
        "destination": "/firebase.html"
      }
    ]
  }
}
